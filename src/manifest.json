{
	"manifest_version": 2,
	"version": "0.1.0",
	"name": "AirChat 0.1.0 Alpha",
	"app": {
		"background": {
			"scripts": [
				"js/EventDispatcher.js",
				
				"js/ChromeCommunicator.js",
				
				"js/AirChat.js",
				"js/AirChatTextView.js",
				"js/AirChatChromeView.js",
				"js/AirChatController.js",
				
				"js/main.js"
			]
		}
	},
	"permissions": [{
		"socket": [
			"udp-bind::5554",
			"udp-send-to::5554",
			"tcp-connect:*:5555",
			"tcp-listen:*:5555",
			"udp-bind::5556",
			"udp-send-to::5556",
			"udp-bind::5559",
			"udp-send-to::5559"
		]
	}],
	"icons": { "16": "media/calculator-16.png", "128": "media/calculator-128.png" }
}
