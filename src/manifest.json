{
	"manifest_version": 2,
	"version": "0.1.0",
	"name": "AirHub 0.1.0 Alpha",
	"app": {
		"background": {
			"scripts": [
				"js/core/view/AirChatTextView.js",
				"js/core/view/AirChatChromeView.js",
				
				"js/core/utils/EventDispatcher.js",
				"js/core/utils/Binary.js",
				
				"js/core/model/ChromeCommunicator.js",
				"js/core/model/Bonjour.js",
				"js/core/model/AirChatModel.js",
				"js/core/model/AirChat.js",
				
				"js/core/controller/AirChatTextController.js",
				
				"js/core/main.js"
			]
		}
	},
	"permissions": [{
		"socket": [
			"udp-bind::5554",
			"udp-send-to::5554"	
		]
	}],
	"icons": { "16": "media/calculator-16.png", "128": "media/calculator-128.png" }
}
