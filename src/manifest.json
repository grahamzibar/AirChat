{
	"manifest_version": 2,
	"version": "0.1.0",
	"name": "AirChat 0.1.0 Alpha",
	"app": {
		"background": {
			"scripts": [
				"js/chrome/ChromeCommunicator.js",
				
				"js/core/AirChat.js",
				"js/core/AirChatTextView.js",
				"js/core/AirChatController.js",
				"js/core/Bonjour.js",
				"js/chrome/AirChatChromeView.js",
				"js/utils/EventDispatcher.js",
				"js/main.js"
			]
		}
	},
	"permissions": [{
		"socket": [
			"udp-bind::5554",
			"udp-send-to::5554",
			"tcp-connect",
			"tcp-listen",
			"udp-bind::5556",
			"udp-send-to::5556",
			"udp-bind::5559",
			"udp-send-to::5559",
			"udp-bind::5561",
			"udp-send-to::5561"			
		]
	}],
	"icons": { "16": "media/calculator-16.png", "128": "media/calculator-128.png" }
}
